<h1>Create new Review!</h1>

<%= form_with model: [@restaurant, @review] do |f| %>
  <%= f.label :rating %>
  <%= f.text_field :rating %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>
<br>
<%= link_to "Go Back to Restaurant Details", restaurant_path(@restaurant) %>
